					<input type="radio" name="CC" value="CC" id="payWithCC" checked> Pay with Credit Card
				</form>
				<div id="cardType">
					<h3>Card Type:</h3>
					<select name="type" class="ui dropdown" id="selectTrim">
						<option value="Visa">Visa</option>
						<option value="AmericanExpress">American Express</option>
						<option value="MasterCard">Master Card</option>
					</select>
				</div>
				<div id="cardNumber">
